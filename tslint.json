{
    "rules": {
        "no-string-throw": true,
        "no-unused-expression": true,
        "no-duplicate-variable": true,
        "curly": false,
        "class-name": true,
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": true,
        "interface-name": true,
        "max-line-length": false
    },
    "defaultSeverity": "warning"
}